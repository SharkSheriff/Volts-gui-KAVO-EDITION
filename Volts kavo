	States = {}
	States.Unloaded = true
	API.Notify("Unloaded Gui, States and stopped most loops.")
	Main:TweenPosition(Main.Position+UDim2.new(0,0,1,0),"In","Back",.45)
	wait(.46)
	TigerAdmin_UI:Destroy()
end,{["Description"] = "Unloads the script",})
