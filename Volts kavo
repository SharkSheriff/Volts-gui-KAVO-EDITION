VsecondSection:NewButton("enable collision", "ButtonInfo", function()
    local Players = game:GetService("Players")

local function enableCollisionForPlayer(player)
    if player.Character then
        for _, part in pairs(player.Character:GetChildren()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end
end

local function onPlayerAdded(player)
    player.CharacterAdded:Connect(function()
        if not game.Workspace:GetPropertyChangedSignal("AllowCollisions"):Connect(function()
            enableCollisionForPlayer(player)
        end) then
            enableCollisionForPlayer(player)
        end
    end)
end

Players.PlayerAdded:Connect(onPlayerAdded)

for _, player in pairs(Players:GetPlayers()) do
    onPlayerAdded(player)
end

end)
