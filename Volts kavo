local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Volts gui NO SKID", "BloodTheme")

local VoltsTab = Window:NewTab("VoltsTab")
local VoltsSection = VoltsTab:NewSection("VoltsSection")
VoltsSection:NewTextBox("Kill player", "TextboxInfo", function(txt)
	
    local targetPlayer = game.Players:FindFirstChild(txt)
    if targetPlayer then
        local localitem = game.Players.LocalPlayer.Backpack["Reach Tool"]
        --local localitemchar = game.Players.LocalPlayer.Character["Reach Tool"]
        local backpack = game.Players.LocalPlayer.Backpack
        local localPlayerChar = game.Players.LocalPlayer.Character
        local localPlayerHum =  game.Players.LocalPlayer.Character.HumanoidRootPart
        local playerChar = targetPlayer.Character
        local mypos = localPlayerHum.Position

        if localPlayerChar and playerChar then
            local targetHumanoidRootPart = playerChar:FindFirstChild("HumanoidRootPart")
            local localHumanoidRootPart = localPlayerChar:FindFirstChild("HumanoidRootPart")
            

            if targetHumanoidRootPart and localHumanoidRootPart then
                workspace.Gravity = 0
                localPlayerHum.CFrame = CFrame.new(9999999, 9999999, 9999999)
                localitem.Parent = localPlayerChar
                wait(0)
                for i = 1, 30 do
                    targetHumanoidRootPart.CFrame = localHumanoidRootPart.CFrame
                    task.wait(0)
                end
                wait(0.6)
                localPlayerHum.CFrame = CFrame.new(mypos)
                workspace.Gravity = 196.2
                local localitemchar = game.Players.LocalPlayer.Character:WaitForChild("Reach Tool")
                localitemchar.Parent = backpack
            end
        end
    else
        print("Player not found!")
    end
end)
VoltsSection:NewButton("Infinite yield", "ButtonInfo", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
